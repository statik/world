{
    "name": "whilp_world",
    "dockerFile": "./img/home/Dockerfile",
    "context": ".",
    "extensions": [
        "bazelbuild.vscode-bazel",
        "eamodio.gitlens",
        "foxundermoon.shell-format",
        "github.vscode-pull-request-github",
        "ms-python.python",
        "ms-vscode.go",
        "redhat.vscode-yaml"
    ],
    "appPort": [
        "8080",
        "1313",
    ],
    "runArgs": [
        "--volume",
        "/var/run/docker.sock:/var/run/docker.sock",
        "--volume",
        "cache:/cache"
    ]
}